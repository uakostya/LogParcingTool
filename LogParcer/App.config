<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="catel">
      <section name="logging" type="Catel.Logging.LoggingConfigurationSection, Catel.Core" />
    </sectionGroup>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <catel>
    <logging>
      <listeners>
        <listener type="Catel.Logging.FileLogListener" FilePath="C:\Log.log" IgnoreCatelLogging="false"
                  IsDebugEnabled="false" IsInfoEnabled="true" IsWarningEnabled="true" IsErrorEnabled="true"/>
      </listeners>
    </logging>
  </catel>
</configuration>
